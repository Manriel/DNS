{
    "name": "Manriel/dns",
    "description": "A PHP library for creating DNS zone files based on RFC1035, the fork of badcow/dns",
    "authors": [
        {
            "name": "Sam Williams",
            "email": "sam@badcow.co"
        }
    ],
    "require": {
        "php": "~5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "friendsofphp/php-cs-fixer": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "Badcow\\DNS\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Badcow\\DNS\\Tests\\": "tests/"
        }
    }
}
